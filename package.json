{
  "name": "pictagram",
  "version": "1.0.0",
  "homepage": "./",
  "private": false,
  "scripts": {
    "appcenter": "appcenter",
    "start": "node app.js",
    "build": "node scripts/build",
    "dev": "node scripts/start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "resources": "capbox resources",
    "android:run": "npm run build && capbox run android",
    "android:build": "npm run build && capbox build android --release",
    "android:distribute:prod": "capbox distribute android --stage production",
    "deploy": "node scripts/deploy/cloud",
    "on:deploy": "node scripts/postdeploy",
    "stats": "react-scripts build --stats && webpack-bundle-analyzer ./build/bundle-stats.json"
  },
  "dependencies": {
    "@capacitor/android": "1.0.0-beta.22",
    "@capacitor/core": "1.0.0-beta.22",
    "@fnando/cpf": "^0.1.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "cordova": "9.0.0",
    "cordova-plugin-android-permissions": "^1.0.0",
    "cordova-plugin-device": "2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova.plugins.diagnostic": "3.7.2",
    "express": "^4.16.4",
    "express-force-https": "^1.0.0",
    "framework7": "^4.2.0",
    "framework7-react": "^4.2.0",
    "hellojs": "^1.18.1",
    "moment-mini": "^2.22.1",
    "react": "^16.7.0",
    "react-app-polyfill": "^1.0.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-lottie": "^1.2.3",
    "react-redux": "^6.0.1",
    "react-scripts": "3.0.1",
    "recordrtc": "^5.5.4",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-filter": "^0.0.18",
    "redux-thunk": "^2.3.0",
    "validator": "^10.11.0",
    "vanilla-masker": "^1.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "1.0.0-beta.22",
    "capbox": "^0.1.4",
    "colors": "^1.3.3",
    "cross-env": "^5.2.0",
    "dotenv": "^7.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "inquirer": "^6.3.1",
    "ip": "^1.1.5",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.16.1",
    "shelljs": "^0.8.3"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "git add"
    ],
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
